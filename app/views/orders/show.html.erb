<table>
  <thead>
    <tr>
      <th colspan="3" scope="col" class="px-6 py-3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order.line_items.each do |order| %>
      <tr>
        <td><%= order.item %></td>
        <td><%= order.quantity %></td>
        <td><%= order.gift_wrap_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_order_path(@order) %> |
<%= link_to 'Back', orders_path %>
